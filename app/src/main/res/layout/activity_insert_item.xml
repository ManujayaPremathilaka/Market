<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".InsertItem">

    <ImageView
        android:id="@+id/homeimage"
        android:layout_width="526dp"
        android:layout_height="900dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:scaleType="fitXY"
        android:src="@drawable/bgapp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.961"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <LinearLayout
        android:id="@+id/heading"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="50dp"
        android:elevation="8dp"
        android:gravity="center"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/additeam"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/additeam"
            android:textColor="#FFF"
            android:textSize="35sp"
            android:textStyle="bold" />

        <EditText
            android:id="@+id/et_IteamId"
            android:layout_width="260dp"
            android:layout_height="30dp"
            android:textColor="#ffffff"
            android:background="#11000000"
            android:layout_below="@+id/iv_user"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="40dp"
            android:ems="10"
            android:hint="ID"
            android:inputType="textPersonName"
            />

        <TextView
            android:id="@+id/tv_IeamId"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/iv_user"
            android:layout_alignStart="@+id/et_name"
            android:layout_alignLeft="@+id/et_name"
            android:layout_marginStart="0dp"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="17dp"
            android:text="@string/tv_IeamId"
            android:textColor="#ffffff"
            android:textSize="16sp" />

        <EditText
            android:id="@+id/et_Name"
            android:textColor="#ffffff"
            android:background="#11000000"
            android:layout_width="260dp"
            android:layout_height="30dp"
            android:layout_below="@+id/et_email"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="40dp"
            android:ems="10"
            android:inputType="textPersonName"
            android:hint="Iteam Name" />

        <TextView
            android:id="@+id/tv_Name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/et_email"
            android:layout_alignStart="@+id/et_name"
            android:layout_alignLeft="@+id/et_name"
            android:layout_marginStart="0dp"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="17dp"
            android:text="@string/tv_Name"
            android:textColor="#ffffff"
            android:textSize="16sp" />
        <!--Nic-->
        <EditText
            android:id="@+id/et_QTY"
            android:textColor="#ffffff"
            android:background="#11000000"
            android:layout_width="260dp"
            android:layout_height="30dp"
            android:layout_below="@+id/et_contactno"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="40dp"
            android:ems="10"
            android:inputType="textPersonName"
            android:hint="Qty" />

        <TextView
            android:id="@+id/tv_QTY"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/et_contactno"
            android:layout_alignStart="@+id/et_name"
            android:layout_alignLeft="@+id/et_name"
            android:layout_marginStart="0dp"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="17dp"
            android:text="@string/tv_QTY"
            android:textColor="#ffffff"
            android:textSize="16sp" />
        <!--vehcal-->
        <EditText
            android:id="@+id/et_price"
            android:textColor="#ffffff"
            android:background="#11000000"
            android:layout_width="260dp"
            android:layout_height="30dp"
            android:layout_below="@+id/et_nic"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="40dp"
            android:ems="10"
            android:inputType="textPersonName"
            android:hint="Price" />

        <TextView
            android:id="@+id/tv_Price"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/et_nic"
            android:layout_alignStart="@+id/et_name"
            android:layout_alignLeft="@+id/et_name"
            android:layout_marginStart="0dp"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="17dp"
            android:text="@string/tv_Price"
            android:textColor="#ffffff"
            android:textSize="16sp"/>

        <Button
            android:id="@+id/Addnew"
            android:layout_width="100dp"
            android:layout_height="30dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="36dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="8dp"
            android:background="@color/gradEnd"
            android:textColor="@color/DarkBlue"
            android:textStyle="italic"
            android:text="Add "
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/button20" />/>


    </LinearLayout>


    <ImageView
        android:id="@+id/imageview5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="272dp"
        android:elevation="6dp"
        android:src="@drawable/clover"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</android.support.constraint.ConstraintLayout>